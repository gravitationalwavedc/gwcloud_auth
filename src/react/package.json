{
  "name": "gwcloud-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve --mode development --hot",
    "start_win": "set \"NODE_ENV=development\" && webpack serve --mode development --hot ",
    "build": "NODE_ENV=production webpack --mode production",
    "build_win": "set \"NODE_ENV=production\" && webpack --mode production",
    "relay": "relay-compiler --src ./src --schema ./data/schema.json --extensions=js --extensions=jsx",
    "test": "jest",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.10",
    "@jest/globals": "^26.6.2",
    "@popperjs/core": "^2.9.1",
    "@testing-library/react": "^11.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^11.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.6.0",
    "camelcase": "^6.2.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-react": "^7.22.0",
    "formik": "^2.2.6",
    "found": "^0.6.0",
    "graphql": "^15.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "jquery": "^3.6.0",
    "linq": "^3.2.3",
    "node-sass": "^5.0.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.2.0",
    "react-relay": "^11.0.0",
    "react-test-renderer": "^17.0.1",
    "relay-compiler": "^11.0.0",
    "relay-runtime": "^11.0.0",
    "relay-test-utils": "^11.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.25.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "yup": "^0.32.9"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "collectCoverage": true,
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9"
  }
}
